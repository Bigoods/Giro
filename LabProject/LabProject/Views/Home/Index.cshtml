@{
    ViewData["Title"] = "Navegar";
}

<script src="~/js/GoogleMap.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCr3s_8juIQR3tv0x_7pt3kBjXk7xz18IM&callback=initMap&libraries=&v=weekly"
        defer></script>

<style type="text/css">
    /* Set the size of the div element that contains the map */
    #map {
        height: 100%;
        width: 100%;
    }

    .gm-fullscreen-control {
        display: none;
    }

    .gm-svpc {
        display: none;
    }
</style>


<!--The div element for the map -->
<div id="map"></div>



<script>
    var Self = "#NavegarBar";
    $(document).ready(function () {
        $(Self).css("background-color", "#121212");
        $(Self).css("border-color", "white");
    });
</script>

